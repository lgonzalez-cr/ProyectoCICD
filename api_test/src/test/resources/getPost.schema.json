{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "default": {},
      "required": [
        "id",
        "title",
        "content",
        "updated_at",
        "created_at",
        "user"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "integer"
        },
        "title": {
          "$id": "#/properties/data/properties/title",
          "type": "string"
        },
        "content": {
          "$id": "#/properties/data/properties/content",
          "type": "string"
        },
        "updated_at": {
          "$id": "#/properties/data/properties/updated_at",
          "type": "integer"
        },
        "created_at": {
          "$id": "#/properties/data/properties/created_at",
          "type": "integer"
        },
        "user": {
          "$id": "#/properties/data/properties/user",
          "type": "object",
          "required": [
            "id",
            "name",
            "email"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/data/properties/user/properties/id",
              "type": "integer"
            },
            "name": {
              "$id": "#/properties/data/properties/user/properties/name",
              "type": "string"
            },
            "email": {
              "$id": "#/properties/data/properties/user/properties/email",
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}